                if Text.Contains(referencia, "DROPROPIZINA") then "DROPROPIZINA" else
                if Text.Contains(referencia, "DROSPIRENONA") then "DROSPIRENONA" else
                if Text.Contains(referencia, "DTPA") then "DTPA" else
                if Text.Contains(referencia, "DULAGLUTIDA") then "DULAGLUTIDA" else
                if Text.Contains(referencia, "DULOXETINA") then "DULOXETINA" else
                if Text.Contains(referencia, "DUPILUMABE") then "DUPILUMABE" else
                if Text.Contains(referencia, "DURVALUMABE") then "DURVALUMABE" else
                if Text.Contains(referencia, "DUTASTERIDA") then "DUTASTERIDA"

                else "Catalogar_Principio_Ativo"
        in
            Result
in
    ClassificarPrincipioAtivo_A_D
